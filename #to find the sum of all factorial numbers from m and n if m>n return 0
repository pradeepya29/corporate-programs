m = int(input("Enter the m value: "))
n = int(input("Enter the n value: "))
def sum_factorials(m, n):
    if m > n:
        return 0  
    total = 0
    for i in range(m, n + 1):
        factorial = 1
        for j in range(1, i + 1):
            factorial *= j
        total += factorial
    return total
print("Sum of factorials =", sum_factorials(m, n))
