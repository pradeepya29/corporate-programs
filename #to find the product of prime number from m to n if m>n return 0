m = int(input("Enter the m value: "))
n = int(input("Enter the n value: "))
def product_prime(m, n):
    if m > n:
        return "invalid"
    product = 1
    prime_found = False
    for i in range(m, n + 1):
        if i > 1:
            for div in range(2, int(i ** 0.5) + 1):
                if i % div == 0:
                    break
            else:
                product *= i
                prime_found = True
                if not prime_found:
                    return "no prime numbers in range"
    return product
print("Product of prime numbers =", product_prime(m, n))
